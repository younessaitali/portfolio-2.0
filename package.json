{
  "name": "template",
  "private": true,
  "version": "0.0.0",
  "packageManager": "pnpm@7.0.0",
  "scripts": {
    "postinstall": "husky install && chmod ug+x .husky/*",
    "preinstall": "npx only-allow pnpm",
    "audit": "run-s audit:*",
    "audit:dev": "pnpm audit --json --dev",
    "audit:prod": "pnpm audit --json --prod",
    "dev": "vite --host",
    "prebuild": "rimraf dist",
    "build:spa": "vue-tsc --noEmit && cross-env NODE_ENV=production vite build",
    "build:ssg": "vue-tsc --noEmit && cross-env NODE_ENV=production vite-ssg build",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "lint": "run-s lint:*",
    "lint:js": "eslint --fix --ext \"**/*.{js,ts,vue,tsx}\"  --ignore-path .eslintignore ",
    "lint:types": "vue-tsc --noEmit",
    "lint:md": "markdownlint --config .markdownlint.json --fix \"**/*.md\"",
    "lint:prettier": "prettier --config ./.prettierrc --write --ignore-path .prettierignore \"**/*.+(js|ts|tsx|vue|json|)\" ",
    "test:e2e": "cypress open",
    "test:unit": "vitest",
    "test:unit:ci": "vitest --run",
    "test:e2e:headless": "cypress run",
    "validate": "npm run lint && npm run build"
  },
  "dependencies": {
    "@pinia/testing": "^0.0.12",
    "@vueuse/core": "^8.5.0",
    "@vueuse/head": "^0.7.6",
    "gsap": "^3.10.4",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.14",
    "prism-theme-vars": "^0.2.3",
    "unocss": "^0.34.0",
    "vue": "^3.2.36",
    "vue-i18n": "^9.1.10",
    "vue-router": "^4.0.15"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@cypress/vite-dev-server": "^2.2.3",
    "@cypress/vue": "^3.1.2",
    "@iconify-json/carbon": "^1.1.5",
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/node": "^17.0.35",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vue/test-utils": "2.0.0",
    "critters": "^0.0.16",
    "cross-env": "^7.0.3",
    "cypress": "^9.6.1",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-vue": "^9.0.1",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "markdown-it-link-attributes": "^4.0.0",
    "markdown-it-prism": "^2.2.4",
    "markdownlint-cli": "^0.31.1",
    "npm-run-all": "^4.1.5",
    "pnpm": "^7.1.3",
    "prettier": "^2.6.2",
    "rimraf": "^3.0.2",
    "typescript": "^4.6.4",
    "unplugin-auto-import": "^0.8.6",
    "unplugin-vue-components": "^0.19.5",
    "vite": "^2.9.9",
    "vite-plugin-inspect": "^0.5.0",
    "vite-plugin-md": "^0.13.1",
    "vite-plugin-pages": "^0.23.0",
    "vite-plugin-pwa": "^0.12.0",
    "vite-plugin-vue-inspector": "^1.0.1",
    "vite-plugin-vue-layouts": "^0.6.0",
    "vite-ssg": "^0.20.0",
    "vite-ssg-sitemap": "^0.2.7",
    "vitest": "^0.12.9",
    "vue-tsc": "^0.34.16"
  },
  "engines": {
    "node": ">=14.17.0"
  }
}
