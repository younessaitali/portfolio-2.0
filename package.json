{
  "private": true,
  "packageManager": "pnpm@7.0.1",
  "scripts": {
    "postinstall": "husky install && chmod ug+x .husky/*",
    "preinstall": "npx only-allow pnpm",
    "audit": "run-s audit:*",
    "audit:dev": "pnpm audit --json --dev",
    "audit:prod": "pnpm audit --json --prod",
    "build": "pnpm lint && nuxi build",
    "dev": "nuxi dev",
    "start": "node .output/server/index.mjs",
    "lint": "run-s lint:*",
    "lint:js": "eslint --fix --ext \"**/*.{js,ts,vue,tsx}\"  --ignore-path .eslintignore ",
    "lint:types": "vue-tsc --noEmit",
    "lint:md": "markdownlint --config .markdownlint.json --fix \"**/*.md\"",
    "lint:prettier": "prettier --config ./.prettierrc --write --ignore-path .prettierignore \"**/*.+(js|ts|tsx|vue|json|)\" ",
    "test:unit": "vitest",
    "test:unit:ci": "vitest --run"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.2",
    "@commitlint/config-conventional": "^17.0.2",
    "@iconify-json/carbon": "^1.1.5",
    "@iconify-json/icon-park-outline": "^1.1.4",
    "@nuxt/content": "^2.0.1",
    "@nuxt/test-utils-edge": "3.0.0-rc.4-27600153.ad42023",
    "@nuxtjs/color-mode": "^3.1.4",
    "@pinia/nuxt": "^0.1.9",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "@unocss/nuxt": "^0.39.3",
    "@vueuse/nuxt": "^8.7.5",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-vue": "^9.1.1",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "markdownlint-cli": "^0.31.1",
    "npm-run-all": "^4.1.5",
    "nuxt": "3.0.0-rc.4",
    "pinia": "^2.0.14",
    "pnpm": "^7.3.0",
    "prettier": "^2.7.1",
    "typescript": "4.7.4",
    "vitest": "^0.16.0",
    "vue-tsc": "^0.38.1"
  },
  "dependencies": {
    "gsap": "^3.10.4"
  }
}
