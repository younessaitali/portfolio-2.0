{
    "private": true,
    "packageManager": "pnpm@7.0.1",
    "scripts": {
        "postinstall": "husky install && chmod ug+x .husky/* && nuxi prepare",
        "preinstall": "npx only-allow pnpm",
        "audit": "run-s audit:*",
        "audit:dev": "pnpm audit --json --dev",
        "audit:prod": "pnpm audit --json --prod",
        "build": "nuxi build",
        "dev": "nuxi dev",
        "start": "node .output/server/index.mjs",
        "lint": "run-s lint:*",
        "lint:js": "eslint --fix --ext \"**/*.{js,ts,vue,tsx}\"  --ignore-path .eslintignore ",
        "lint:types": "vue-tsc --noEmit",
        "lint:md": "markdownlint --config .markdownlint.json --fix \"**/*.md\"",
        "lint:prettier": "prettier --config ./.prettierrc --write --ignore-path .prettierignore \"**/*.+(js|ts|tsx|vue|json|)\" ",
        "test:unit": "vitest",
        "test:unit:ci": "vitest --run"
    },
    "devDependencies": {
        "@commitlint/cli": "^17.0.3",
        "@commitlint/config-conventional": "^17.0.3",
        "@iconify-json/carbon": "^1.1.7",
        "@iconify-json/icon-park-outline": "^1.1.5",
        "@nuxt/content": "^2.0.1",
        "@nuxt/test-utils-edge": "3.0.0-rc.6-27646047.133723f",
        "@nuxtjs/color-mode": "^3.1.4",
        "@pinia/nuxt": "^0.3.1",
        "@types/nprogress": "^0.2.0",
        "@typescript-eslint/eslint-plugin": "^5.31.0",
        "@typescript-eslint/parser": "^5.31.0",
        "@unocss/nuxt": "^0.44.7",
        "@vee-validate/zod": "^4.6.2",
        "@vueuse/nuxt": "^8.9.4",
        "eslint": "^8.20.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-vue": "^9.3.0",
        "husky": "^8.0.1",
        "lint-staged": "^13.0.3",
        "markdownlint-cli": "^0.32.1",
        "npm-run-all": "^4.1.5",
        "nuxt": "3.0.0-rc.6",
        "nuxt-typed-router": "^1.1.2",
        "pinia": "^2.0.17",
        "pnpm": "^7.6.0",
        "prettier": "^2.7.1",
        "typescript": "4.7.4",
        "vee-validate": "^4.6.2",
        "vitest": "^0.19.1",
        "vue-tsc": "^0.39.0",
        "zod": "^3.17.10",
        "vite": "3.0.4"
    },
    "dependencies": {
        "gsap": "^3.10.4"
    }
}
